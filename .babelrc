{
    "presets": [
        [
            "@babel/env",
            {
                "corejs": {
                    "version": 3,
                    "proposals": true,
                },
                "useBuiltIns": "false",
                // "targets": {
                //   "browsers": ["last 2 versions"],
                // },
                "debug": false,
            }
        ],
    ],
    "plugins": [
        [
            "@babel/plugin-proposal-private-methods",
            {
                "loose": false,
            }
        ],
        [
            "@babel/plugin-proposal-class-properties",
            {
                "loose": false,
            },
        ],
    ],
}